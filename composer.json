{
    "name": "surangapg/heavyd-global",
    "description": "Contains a helper to handle global heavyd commands.",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
      {
        "type": "vcs",
        "url":  "git@github.com:SurangaPG/heavyd-components.git"
      }
    ],
    "autoload": {
        "psr-4": {
            "surangapg\\HeavydGlobal\\Test\\": "tests/Test",
            "surangapg\\HeavydGlobal\\": "src"
        }
    },
    "require": {
        "padraic/phar-updater": "^1.0",
        "guzzlehttp/guzzle": "^6.2",
        "tlr/html-table-builder": "^1.1",
        "symfony/console": "^3.4",
        "symfony/yaml": "^3.4",
        "symfony/filesystem": "^3.4",
        "surangapg/heavyd-components": "dev-master"
    },
    "config": {
      "platform": {
        "php": "7.0.22"
      }
    },
    "bin": [
        "bin/heavyd"
    ]
}
